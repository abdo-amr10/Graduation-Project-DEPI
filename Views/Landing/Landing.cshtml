  <script>
    // Initialize AOS
    AOS.init({
      duration: 1000,
      once: true
    });

    // Navbar scroll effect
    window.addEventListener('scroll', function() {
      const navbar = document.querySelector('.ftco-navbar-light');
      if (window.scrollY > 50) {
        navbar.classList.add('navbar-scrolled');
      } else {
        navbar.classList.remove('navbar-scrolled');
      }
    });

    // Smooth scroll for nav links (robust): scroll, close mobile menu, handle missing targets
    document.querySelectorAll('a[href^="#"]').forEach(anchor => {
      anchor.addEventListener('click', function (e) {
        const hash = this.getAttribute('href');
        if (!hash || hash === '#') return;
        const target = document.querySelector(hash);
        if (!target) return;
        e.preventDefault();
        // Smooth scroll
        target.scrollIntoView({ behavior: 'smooth', block: 'start' });

        // Force active state on clicked nav link
        document.querySelectorAll('.nav-link').forEach(link => link.classList.remove('active'));
        this.classList.add('active');

        // If mobile navbar is open, close it (Bootstrap collapse)
        try {
          const bsCollapseEl = document.getElementById('navbarSupportedContent');
          if (bsCollapseEl && bsCollapseEl.classList.contains('show') && typeof bootstrap !== 'undefined') {
            const bs = new bootstrap.Collapse(bsCollapseEl);
            bs.hide();
          }
        } catch (err) {
          // ignore if bootstrap not available
        }
      });
    });

    // Active nav link on scroll
    const sections = document.querySelectorAll('section[id]');
    const navLinks = document.querySelectorAll('.nav-link');
    window.addEventListener('scroll', () => {


      const viewportCenter = window.pageYOffset + (window.innerHeight / 2);
      const viewportBottom = window.pageYOffset + window.innerHeight;
      let current = '';
      let found = false;
      sections.forEach((section, idx) => {
        const sectionTop = section.offsetTop;
        const sectionHeight = section.offsetHeight;
        if (viewportCenter >= sectionTop && viewportCenter < (sectionTop + sectionHeight)) {
          current = section.getAttribute('id');
          found = true;
        }
      });

      // If viewport bottom is at or past Contact section top, always set current to contact
      const contactSection = document.getElementById('contact');
      if (contactSection && viewportBottom >= contactSection.offsetTop) {
        current = 'contact';
        found = true;
      }

      // Fallback: if none matched (very top), set to home
      if (!found && sections.length) {
        current = sections[0].getAttribute('id') || '';
      }

      navLinks.forEach(link => {
        link.classList.remove('active');
        if (link.getAttribute('href') === `#${current}`) {
          link.classList.add('active');
        }
      });

      // Toggle navbar-black only after we've fully left the hero section
      const navbar = document.querySelector('.ftco-navbar-light');
      const home = document.querySelector('#home');
      if (navbar && home) {
        const homeBottom = home.offsetTop + home.offsetHeight - 8; // small buffer
        if (window.pageYOffset > homeBottom) {
          navbar.classList.add('navbar-black');
        } else {
          navbar.classList.remove('navbar-black');
        }
      }
    });

